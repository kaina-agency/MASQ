{
  "name": "masq",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "MASQ",
  "cordovaId": "com.lovallo.masq",
  "capacitorId": "",
  "author": "James Lovallo <patrickonparker@gmail.com>",
  "private": true,
  "scripts": {
    "Dev": "netlify dev -c 'quasar d'",
    "Dev_PWA": "netlify dev -c 'quasar d -m pwa'",
    "Prod_PWA": "netlify dev -c 'quasar b -m pwa'",
    "Dev_Electron": "netlify dev -c 'quasar d -m electron'",
    "Prod_Electron": "netlify dev -c 'quasar b -m electron'",
    "Dev_iOS": "netlify dev -c 'quasar d -m ios'",
    "Prod_iOS": "netlify dev -c 'quasar b -m ios'",
    "Dev_Android": "netlify dev -c 'quasar d -m android'",
    "Prod_Android": "netlify dev -c 'quasar b -m android'"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.2",
