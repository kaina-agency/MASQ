    "quasar": "^1.3.0",
    "storyblok-js-client": "^2.0.10",
    "storyblok-vue": "^1.0.5",
    "vue": "^2.6.10",
    "vue-fragment": "^1.5.1",
    "webfontloader": "^1.6.28",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.2.3",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.2",
