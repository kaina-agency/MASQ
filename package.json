    "dotenv": "^8.2.0",
    "node-sass": "^4.13.0",
    "portal-vue": "^2.1.6",
    "quasar": "^1.4.4",
    "storyblok-js-client": "^2.0.10",
    "storyblok-vue": "^1.0.5",
    "vue": "^2.6.10",
    "vue-carousel-3d": "^0.2.0",
    "vue-fragment": "^1.5.1",
    "vue-packer": "^0.3.0",
    "webfontloader": "^1.6.28",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.2.4",
