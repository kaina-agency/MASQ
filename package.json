{
  "name": "masq",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "MASQ",
  "cordovaId": "com.lovallo.masq",
  "capacitorId": "",
  "author": "James Lovallo <patrickonparker@gmail.com>",
  "private": true,
  "scripts": {
    "↻⠀Switch Projects": "netlify unlink && netlify link && netlify dev -c 'node masq.icon.js'",
    "▲⠀Publish⠀to⠀Netlify": "netlify dev -c 'quasar b -m pwa' && netlify deploy -p",
    "▶⠀Start⠀Dev⠀Server": "netlify dev -c 'quasar d'",
    "⠀⠀├⠀Platforms": "",
    "⠀⠀⠀⠀├⠀Dev:⠀PWA": "netlify dev -c 'quasar d -m pwa'",
    "⠀⠀⠀⠀├⠀Build:⠀PWA": "netlify dev -c 'quasar b -m pwa'",
    "⠀⠀⠀⠀├⠀Dev:⠀Electron": "netlify dev -c 'quasar d -m electron'",
    "⠀⠀⠀⠀├⠀Build:⠀Electron": "netlify dev -c 'quasar b -m electron'",
    "⠀⠀⠀⠀├⠀Dev:⠀iOS": "netlify dev -c 'quasar d -m ios'",
    "⠀⠀⠀⠀├⠀Build:⠀iOS": "netlify dev -c 'quasar b -m ios'",
    "⠀⠀⠀⠀├⠀Dev:⠀Android": "netlify dev -c 'quasar d -m android'",
    "⠀⠀⠀⠀├⠀Build:⠀Android": "netlify dev -c 'quasar b -m android'"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.3",
